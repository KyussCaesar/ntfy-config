<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>sh.ntfy.server</string>

    <key>ProgramArguments</key>
    <array>
        <string>__NTFY_SERVER_BIN__</string>
        <string>serve</string>
        <string>--config</string>
        <string>__CONFIG_FILE__</string>
    </array>

    <key>RunAtLoad</key>
    <true/>

    <key>KeepAlive</key>
    <true/>

    <key>StandardOutPath</key>
    <string>__LOG_DIR__/ntfy.log</string>

    <key>StandardErrorPath</key>
    <string>__LOG_DIR__/ntfy.error.log</string>

    <key>WorkingDirectory</key>
    <string>__DATA_DIR__</string>
</dict>
</plist>
